{
	"id": "case_study",
	"label": "Case Study",
	"format": "page",
	"repeatable": true,
	"status": true,
	"json": {
		"Main": {
			"uid": {
				"config": {
					"label": "UID"
				},
				"type": "UID"
			},
			"title": {
				"type": "Text",
				"config": {
					"label": "Title",
					"placeholder": ""
				}
			},
			"protected": {
				"type": "Boolean",
				"config": {
					"label": "protected",
					"default_value": false,
					"placeholder_true": "true",
					"placeholder_false": "false"
				}
			},
			"highlight_image": {
				"type": "Image",
				"config": {
					"label": "Highlight Image",
					"constraint": {},
					"thumbnails": []
				}
			},
			"image_fill": {
				"type": "Boolean",
				"config": {
					"label": "Image Fill",
					"default_value": false,
					"placeholder_true": "true",
					"placeholder_false": "false"
				}
			},
			"date": {
				"type": "Date",
				"config": {
					"label": "Date",
					"placeholder": ""
				}
			},
			"affiliation": {
				"type": "Link",
				"config": {
					"label": "Affiliation",
					"select": "document",
					"customtypes": ["affiliation"]
				}
			},
			"responsibilities": {
				"type": "Group",
				"config": {
					"label": "Responsibilities",
					"fields": {
						"skill": {
							"type": "Select",
							"config": {
								"label": "Skill",
								"placeholder": "",
								"options": ["UI", "UX", "Research", "Strategy", "Data Viz"]
							}
						}
					}
				}
			},
			"slices": {
				"type": "Slices",
				"fieldset": "Slice Zone",
				"config": {
					"choices": {
						"embed_block": {
							"type": "SharedSlice"
						},
						"image_block": {
							"type": "SharedSlice"
						},
						"text_block": {
							"type": "SharedSlice"
						}
					}
				}
			}
		},
		"SEO & Metadata": {
			"meta_description": {
				"config": {
					"label": "Meta Description",
					"placeholder": "A brief summary of the page"
				},
				"type": "Text"
			},
			"meta_image": {
				"config": {
					"constraint": {
						"height": 1260,
						"width": 2400
					},
					"label": "Meta Image",
					"thumbnails": []
				},
				"type": "Image"
			},
			"meta_title": {
				"config": {
					"label": "Meta Title",
					"placeholder": "A title of the page used for social media and search engines"
				},
				"type": "Text"
			}
		}
	}
}
